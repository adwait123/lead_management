DATABASE_URL=sqlite:///./ailead.db
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key

# LiveKit Configuration for Outbound and Inbound Calling
LIVEKIT_URL=wss://your-livekit-server
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret
SIP_OUTBOUND_TRUNK_ID=your-sip-trunk-id
SIP_INBOUND_TRUNK_ID=your-inbound-sip-trunk-id

# Agent API Keys
DEEPGRAM_API_KEY=your-deepgram-api-key
CARTESIA_API_KEY=your-cartesia-api-key
USE_CARTESIA_TTS=true
AGENT_NAME=outbound_call_agent

# Outbound Web Service Configuration (Alternative to LiveKit)
OUTBOUND_WEB_SERVICE_URL=https://outbound-call-support-webhook.onrender.com/api/v1/dispatch-call
OUTBOUND_WEB_SERVICE_API_KEY=secure-api-key-change-this-in-production